{
  "name": "react-typescript-tailwind-starter",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint -c .eslintrc.cjs \"src/**/*.ts\" \"src/**/*.tsx\" --max-warnings=0",
    "lint:fix": "eslint --fix -c .eslintrc.cjs \"src/**/*.ts\" \"src/**/*.tsx\"",
    "format:fix": "pretty-quick --pattern \"src/**/*.{ts,tsx}\"",
    "format:check": "pretty-quick --staged --pattern \"src/**/*.{ts,tsx}\"",
    "pre-commit": "npm run lint && npm run format:check",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.50.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "postcss": "^8.4.31",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "license": "MIT"
}
